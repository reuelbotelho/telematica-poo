import java.util.Scanner;

public class ComparacaoIdade {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Digite a primeira idade: ");
        int idade1 = scanner.nextInt();
        System.out.print("Digite a segunda idade: ");
        int idade2 = scanner.nextInt();

        if (idade1 > idade2) {
            System.out.println("A primeira idade é maior.");
        } else if (idade1 < idade2) {
            System.out.println("A segunda idade é maior.");
        } else {
            System.out.println("As idades são iguais.");
        }
    }
}